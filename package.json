{
  "name": "additween-mocks",
  "version": "2.1.1",
  "description": "Mocks for the additween animation library",
  "main": "dist/additween-mocks.js",
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "webpack",
    "dev": "webpack --watch",
    "lint": "eslint --ext .js .",
    "test:lint": "npm run lint",
    "test:sources": "karma start karma.conf.js",
    "test:typings": "./node_modules/.bin/_mocha -- test/typings.spec.js",
    "test": "run-p test:*",
    "coverage": "cat coverage/*/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhovhannes/additween-mocks.git"
  },
  "keywords": [
    "additween",
    "mocks",
    "stub",
    "additive",
    "tweening",
    "animation",
    "state",
    "easing"
  ],
  "author": "Hovhannes Babayan <bhovhannes@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhovhannes/additween-mocks/issues"
  },
  "homepage": "https://github.com/bhovhannes/additween-mocks#readme",
  "devDependencies": {
    "codecov": "2.3.0",
    "eslint": "4.8.0",
    "eslint-loader": "1.9.0",
    "expect.js": "0.3.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "karma": "1.7.1",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.4",
    "mocha": "4.0.0",
    "npm-run-all": "4.1.1",
    "phantomjs-prebuilt": "2.1.15",
    "typescript": "2.5.3",
    "typescript-definition-tester": "0.0.5",
    "webpack": "3.6.0"
  },
  "dependencies": {
    "additween": "~2.1.0"
  }
}
